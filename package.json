{
  "name": "@agilepixel/pixelate",
  "version": "0.9.1",
  "description": "Modified version of the build scripts from the Sage Wordpress theme to make is portable for most use cases",
  "main": "index.js",
  "scripts": {
    "lint": "eslint build --quiet",
    "test": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --mode=development --config=build/webpack.config.js",
