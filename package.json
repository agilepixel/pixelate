    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-plugin-import": "^2.25.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-node": "^10.9.1",
    "vue": "^3.2.13",
    "webpack": "^5.94.0"
  }
}